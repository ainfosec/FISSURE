<?xml version="1.0"?>
<block>
  <name>Extract System Info</name>
  <key>gsm_extract_system_info</key>
  <import>import grgsm</import>
  <make>grgsm.extract_system_info()</make>

  <sink>
    <name>msgs</name>
    <type>message</type>
  </sink>

  <sink>
    <name>bursts</name>
    <type>message</type>
  </sink>
  
</block>
